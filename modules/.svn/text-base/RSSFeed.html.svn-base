## -*- coding: utf-8 -*-
<%inherit file="/module_base.html" />
<%def name="content()">
% if len(feed_list) == 0:
<p>No entries to display</p>
% else:
<dl>
% for feed in feed_list:
<dd><a href="${feed['link']}">
${feed['title']}</a></dd>
<%
# Convert published time to local time
from time import ctime
published = ctime(feed['published'])
%>
<dd>Posted on <strong>${published}</strong></dd>
<dd>
${feed['content']}
</dd>
% endfor
</dl>
% endif
</%def>
